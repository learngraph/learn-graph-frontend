{
  "name": "learn-graph-frontend-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "devPublic": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test-watch": "vitest",
    "fmt": "prettier --write .",
    "init-git": "ln -sf ../../tools/pre-commit .git/hooks/pre-commit",
    "build-arm": "docker build -t test-frontend --build-arg='NGINX_IMAGE=arm64v8/nginx:stable-alpine' .",
    "build-amd": "docker build -t test-frontend --build-arg='NGINX_IMAGE=nginx:1.12-alpine' ."
  },
  "dependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.23.4",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/orbitron": "^5.1.1",
    "@lexical/react": "^0.13.1",
    "@mui/icons-material": "^5.16.13",
    "@mui/material": "^5.15.10",
    "@tailwindcss/vite": "^4.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/mui-image": "^1.0.5",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-fetch": "^4.0.0",
    "i18next": "^23.8.2",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lexical": "^0.13.1",
    "mui-image": "^1.0.7",
    "posthog-js": "^1.232.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-i18next": "^14.0.5",
    "react-icons": "^5.4.0",
    "react-router-dom": "^6.22.0",
    "tailwindcss": "^4.0.17",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": ">=5.1.7",
    "vite-plugin-sitemap": "^0.7.1",
    "vitest": "^3.0.9",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.2.5"
  },
  "proxy": "http://backend:8080"
}
